def password(x):
    import random
    x=[]
    z=random.randrange(7,11)
    w=random.randrange(33,126)
    m=chr(w)
    x.append(m)
    while len(x)>0: 
        w=random.randrange(33,126)
        m=chr(w)
        x.append(m)
        if len(x)==z:
            return ''.join(x)

y=password
    
def main():
    print (password(y))
main()
